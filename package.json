{
  "name": "dk-redux",
  "description": "A reduxed remixed remake of Donkey Kong, the original arcade game from 1981, built with React, Redux Toolkit and SCSS.",
  "version": "0.0.1",
  "type": "module",
  "homepage": "http://fer-nando-machado.github.io/dk-redux",
  "author": "Fernando Machado",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fer-nando-machado/dk-redux.git",
    "link": "https://github.com/fer-nando-machado/dk-redux"
  },
  "private": false,
  "keywords": [
    "react",
    "redux",
    "redux-toolkit",
    "scss",
    "game",
    "donkey kong"
  ],
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-deutschbox": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.1.1",
    "sass": "^1.77.8",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
